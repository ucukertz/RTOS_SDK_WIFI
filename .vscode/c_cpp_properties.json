            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/build/include/*",
                "~/esp/ESP8266_RTOS_SDK/components/**",
                "~/esp/ESP8266_RTOS_SDK/components/freertos/port/esp8266/include",
